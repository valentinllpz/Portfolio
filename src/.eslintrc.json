{
	"env": {
	  "browser": true,
	  "es6": true,
	  "jest": true
	},
	"extends": [
	  "next/core-web-vitals"
	],
	"plugins": ["react", "@typescript-eslint"],
	"settings": {
	  "react": {
		"pragma": "React",
		"version": "detect"
	  }
	},
	"rules": {
	  "@typescript-eslint/explicit-function-return-type": 0,
	  "@typescript-eslint/explicit-module-boundary-types": 0,
	  "react/react-in-jsx-scope": 0
	}
  }
  